<#list literalProperties as property>
<#if property.single>

  public ${property.simpleReturnType} get${property.relationName}() {
    return coinsModel.getLiteralValue(getUri(), "${property.relationUri}", ${property.returnType}.class);
  }
  public void set${property.relationName}(${property.simpleReturnType} object) {
    coinsModel.setLiteralValue(getUri(), "${property.relationUri}", object);
  }
<#else>

  public Iterator<${property.returnType}> get${property.relationName}() {
    return coinsModel.getLiteralValues(getUri(), "${property.relationUri}", ${property.returnType}.class);
  }
  public void add${property.relationName}(${property.simpleReturnType} object) {
    coinsModel.addLiteralValue(getUri(), "${property.relationUri}", object);
  }
</#if>

  public void remove${property.relationName}(${property.simpleReturnType} object) {
    coinsModel.removeLiteralValue(getUri(), "${property.relationUri}", object);
  }
  public void removeAll${property.relationName}() {
    coinsModel.removeAllStatements(getUri(), "${property.relationUri}");
  }
</#list>