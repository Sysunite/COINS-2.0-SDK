<#list literalProperties as property>
<#if property.single>

  public ${property.simpleReturnType} get${property.relationName}() {
    return model.getLiteralValue(getUri(), "${property.relationUri}", ${property.returnType}.class);
  }
  public void set${property.relationName}(${property.simpleReturnType} object) {
    model.setLiteralValue(getUri(), "${property.relationUri}", object);
  }
<#else>

  public Iterator<${property.returnType}> get${property.relationName}() {
    return model.getLiteralValues(getUri(), "${property.relationUri}", ${property.returnType}.class);
  }
  public void add${property.relationName}(${property.simpleReturnType} object) {
    model.addLiteralValue(getUri(), "${property.relationUri}", object);
  }
</#if>

  public void remove${property.relationName}(${property.simpleReturnType} object) {
    model.removeLiteralValue(getUri(), "${property.relationUri}", object);
  }
  public void removeAll${property.relationName}() {
    model.removeAllStatements(getUri(), "${property.relationUri}");
  }
</#list>